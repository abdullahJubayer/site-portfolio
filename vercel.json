{
  "version": 1,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "build/client/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/build/client/assets/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/build/client/favicon.ico"
    },
    {
      "src": "/(.*\\.css)",
      "dest": "/build/client/$1"
    },
    {
      "src": "/(.*\\.js)",
      "dest": "/build/client/$1"
    },
    {
      "src": "/(.*\\.png|.*\\.jpg|.*\\.svg|.*\\.webp)",
      "dest": "/build/client/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}
